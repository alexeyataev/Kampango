<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>Send_reunion_session_email_to_attendee</fullName>
        <description>Send reunion session email to attendee in public venue</description>
        <protected>false</protected>
        <recipients>
            <field>Email</field>
            <type>email</type>
        </recipients>
        <senderType>DefaultWorkflowUser</senderType>
        <template>NCT_Templates/Reunion_Session_Attendees_Public_Venue_Email</template>
    </alerts>
    <alerts>
        <fullName>Send_reunion_session_email_to_attendee_in_host_venue</fullName>
        <description>Send reunion session email to attendee in host venue</description>
        <protected>false</protected>
        <recipients>
            <field>Email</field>
            <type>email</type>
        </recipients>
        <senderType>DefaultWorkflowUser</senderType>
        <template>NCT_Templates/Reunion_Session_Attendees_Not_Client_Host_Email</template>
    </alerts>
    <alerts>
        <fullName>Send_reunion_session_email_to_practitioner</fullName>
        <description>Send reunion session email to practitioner</description>
        <protected>false</protected>
        <recipients>
            <field>Email</field>
            <type>email</type>
        </recipients>
        <senderType>DefaultWorkflowUser</senderType>
        <template>NCT_Templates/Reunion_Session_Course_Leader_Email</template>
    </alerts>
</Workflow>
