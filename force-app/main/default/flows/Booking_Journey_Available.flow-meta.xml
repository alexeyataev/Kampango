                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>session.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Provisional</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Session_Loop</targetReference>
            </connector>
            <label>true</label>
        </rules>
    </decisions>
