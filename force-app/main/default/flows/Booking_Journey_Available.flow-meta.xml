    <decisions>
        <name>Booking_Journey_Available_Decision</name>
        <label>Booking Journey Available Decision</label>
        <locationX>675</locationX>
        <locationY>236</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Link_Is_Expired</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>curBooking.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Expired</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>curCourse.Remaining_Places__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Link_Expired</targetReference>
            </connector>
            <label>Link Is Expired</label>
        </rules>
        <rules>
            <name>Course_Is_Cancelled</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>curCourse.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Cancelling</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>curCourse.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Cancelled</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Cancelled</targetReference>
            </connector>
            <label>Course Is Cancelled</label>
        </rules>
        <rules>
            <name>Course_Is_Confirmed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>curBooking.Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Confirmed</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Confirmed_Booking</targetReference>
            </connector>
            <label>Course Is Confirmed</label>
        </rules>
    </decisions>
    <decisions>
        <name>Doest_list_contain_this_id</name>
        <label>Doest list contain this id?</label>
        <locationX>562</locationX>
        <locationY>756</locationY>
        <defaultConnector>
            <targetReference>Add_distinct_venue_id</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>True</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>venueIdsDistinct</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>session.Venue__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Session_Loop</targetReference>
            </connector>
            <label>True</label>
        </rules>
    </decisions>
