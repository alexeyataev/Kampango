<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>47.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Custom Button</masterLabel>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property name="label" label="Label" type="String" default="New"/>
            <property name="align" label="Align" type="String" default="Right" datasource="Right,Left"/>
            <property name="url" label="URL" type="String" default="/home"/>
            <property name="variant" label="Variant" type="String" default="Neutral" datasource="Neutral,Base,Brand,Brand-outline,Destructive"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
