<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CountryEditFromPicklistOnly</fullName>
    <active>true</active>
    <description>Country must be updated from Country picklist</description>
    <errorConditionFormula>ISCHANGED(MailingCountry) &amp;&amp; MailingCountry != TEXT(Country__c)</errorConditionFormula>
    <errorMessage>Please use the country picklist.</errorMessage>
</ValidationRule>
