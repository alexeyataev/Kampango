<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Zumzum__AccttType</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Type  , &#39;Customer&#39;), ISBLANK(AccountNumber) )
 || 
AND(ISPICKVAL(Type  , &#39;Supplier&#39;), ISBLANK(AccountNumber) )</errorConditionFormula>
    <errorDisplayField>AccountNumber</errorDisplayField>
    <errorMessage>You must enter Account Number</errorMessage>
</ValidationRule>
