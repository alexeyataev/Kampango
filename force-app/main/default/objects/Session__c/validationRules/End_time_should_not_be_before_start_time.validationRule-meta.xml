<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_time_should_not_be_before_start_time</fullName>
    <active>true</active>
    <errorConditionFormula>End__c &lt;= Start__c &amp;&amp; NOT($Permission.Override_Session_Validation)</errorConditionFormula>
    <errorDisplayField>End__c</errorDisplayField>
    <errorMessage>Session end time must be later than session start time.</errorMessage>
</ValidationRule>
