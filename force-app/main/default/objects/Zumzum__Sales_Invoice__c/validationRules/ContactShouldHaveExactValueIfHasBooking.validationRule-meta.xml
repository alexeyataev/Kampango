<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ContactShouldHaveExactValueIfHasBooking</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISBLANK( Booking__c )) &amp;&amp; 
Zumzum__Contact__c &lt;&gt; Booking__r.Primary_Contact__c</errorConditionFormula>
    <errorDisplayField>Zumzum__Contact__c</errorDisplayField>
    <errorMessage>This field should have the same value as Booking Primary Contact</errorMessage>
</ValidationRule>
