<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Delivery_Mechanism_Populating</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
AND(RecordType.DeveloperName == &#39;Virtual&#39;, ISNULL(TEXT(Delivery_Mechanism__c))),
AND(RecordType.DeveloperName != &#39;Virtual&#39;,
NOT(ISNULL(TEXT(Delivery_Mechanism__c))))
)</errorConditionFormula>
    <errorMessage>Delivery Mechanism must be populated for Virtual venues only</errorMessage>
</ValidationRule>
