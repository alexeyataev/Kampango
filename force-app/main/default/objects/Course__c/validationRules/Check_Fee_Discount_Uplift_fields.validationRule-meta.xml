<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Fee_Discount_Uplift_fields</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Practitioner_Fee_Discount__c  &lt;&gt; 0,
Practitioner_Fee_Uplift__c  &lt;&gt; 0,
NOT($Permission.Override_Course_Validation)
)</errorConditionFormula>
    <errorMessage>Please only enter one of Fee Discount or Fee Uplift percentage.</errorMessage>
</ValidationRule>
