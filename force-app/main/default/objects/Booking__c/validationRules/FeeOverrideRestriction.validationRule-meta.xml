<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FeeOverrideRestriction</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISBLANK( FeeOverride__c )) &amp;&amp; ISPICKVAL(FeeOverrideReason__c, &quot;&quot;)</errorConditionFormula>
    <errorDisplayField>FeeOverrideReason__c</errorDisplayField>
    <errorMessage>Please fill in Fee Override Reason.</errorMessage>
</ValidationRule>
