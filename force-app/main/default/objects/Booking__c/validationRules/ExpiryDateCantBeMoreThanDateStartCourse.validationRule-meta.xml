<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ExpiryDateCantBeMoreThanDateStartCourse</fullName>
    <active>true</active>
    <errorConditionFormula>Reservation_Expiry_Date__c &gt;= Course__r.Start_Date__c &amp;&amp;
NOT($Permission.Override_Booking_Validation)</errorConditionFormula>
    <errorDisplayField>Reservation_Expiry_Date__c</errorDisplayField>
    <errorMessage>Expiry date cannot be greater than course start date</errorMessage>
</ValidationRule>
