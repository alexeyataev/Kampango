<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MandatoryPartnerContactField</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISBLANK(Partner_Contact__r.Email),
Partner_Contact__c != NULL,
NOT($Permission.Override_Booking_Validation)
)</errorConditionFormula>
    <errorMessage>Please review partner contact. Email is required.</errorMessage>
</ValidationRule>