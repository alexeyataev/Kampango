    <validationRules>
        <fullName>Capacity_Psa_validation_on_Status</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
ISPICKVAL(Status__c, &apos;Closed&apos;),
ISPICKVAL(Status__c, &apos;Cancelled&apos;),
ISPICKVAL(Status__c, &apos;In Progress&apos;)
)
 &amp;&amp; 

OR(
ISPICKVAL(PSA_Office__c, &quot;1C&quot;),ISPICKVAL(PSA_Office__c, &quot;2A&quot;),ISPICKVAL(PSA_Office__c, &quot;4D&quot;),ISPICKVAL(PSA_Office__c, &quot;3C&quot;)
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>PSA Assignments can not be modified for the current status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Numer_of_Places_against_Practition</fullName>
        <active>true</active>
        <description>Limit number of places on course to the number the practitioner is authorised to have</description>
        <errorConditionFormula>NOT(ISBLANK(Practitioner__r.Maximum_Clients_per_Course__c)) &amp;&amp;  Places__c &gt; Practitioner__r.Maximum_Clients_per_Course__c</errorConditionFormula>
        <errorDisplayField>Places__c</errorDisplayField>
        <errorMessage>Too many course places for the selected practitioner</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Course_End_date</fullName>
        <active>true</active>
        <description>Validation rule to make sure that end date is captured for Antenatal Course</description>
        <errorConditionFormula>AND(ISPICKVAL(Course_Name__c, &quot;Antenatal&quot;),ISNULL(Course_End_Date__c))</errorConditionFormula>
        <errorDisplayField>Course_End_Date__c</errorDisplayField>
        <errorMessage>Course End Date field cannot be left blank for Antenatal Course</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Course_Start_date</fullName>
        <active>true</active>
        <description>Validation rule to make sure that start date is captured for Post Natal Courses</description>
        <errorConditionFormula>AND(ISPICKVAL(Course_Name__c, &quot;Postnatal&quot;),ISNULL(Course_Start_Date__c))</errorConditionFormula>
        <errorDisplayField>Course_Start_Date__c</errorDisplayField>
        <errorMessage>Course Start Date field cannot be left blank for Post Natal Courses</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Course_Sub_Category</fullName>
        <active>true</active>
        <description>Sub Category field should be dependent Picklist of Category and required if Category is selected</description>
        <errorConditionFormula>ISPICKVAL(Course_Sub_Category__c, &apos;None&apos;) || ISPICKVAL(Course_Category__c , &apos;None&apos;)</errorConditionFormula>
        <errorDisplayField>Course_Sub_Category__c</errorDisplayField>
        <errorMessage>Required field is missing: Course Sub Category; Course Category</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validation_for_Fee_field_in_Course</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Fee__c )</errorConditionFormula>
        <errorDisplayField>Fee__c</errorDisplayField>
        <errorMessage>Please fill Fee field</errorMessage>
    </validationRules>
</CustomObject>
