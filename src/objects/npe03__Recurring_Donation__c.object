    <validationRules>
        <fullName>Cancellation_reason_should_be_mandatory</fullName>
        <active>true</active>
        <description>Cancellation reason should be mandatory when the Membership Status updated to Cancelled or Cancelling</description>
        <errorConditionFormula>OR(
ISPICKVAL(Membership_Status__c, &apos;Cancelled&apos;),
ISPICKVAL(Membership_Status__c, &apos;Cancelling&apos;)
)
 &amp;&amp; 
OR(
ISBLANK(TEXT(Cancellation_Reason__c))
)</errorConditionFormula>
        <errorDisplayField>Cancellation_Reason__c</errorDisplayField>
        <errorMessage>You must select Cancellation reason</errorMessage>
    </validationRules>
