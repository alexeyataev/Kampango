BLANKVALUE(cpm__End_Date__c,
IF(
  AND(
    MONTH( BLANKVALUE(cpm__Last_Used__c, cpm__Date_Signed__c) ) = 2,
    DAY( BLANKVALUE(cpm__Last_Used__c, cpm__Date_Signed__c) ) = 29,
    NOT( 
