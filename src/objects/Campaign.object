    <validationRules>
        <fullName>Branch_Mandatory_for_Sling_Meet_Type</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND( ISPICKVAL( Type , &apos;Sling Meet&apos;) , ISBLANK( Branch__c ) ) , true, false)</errorConditionFormula>
