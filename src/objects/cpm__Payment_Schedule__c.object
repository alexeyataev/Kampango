    <validationRules>
        <fullName>cpm__Collection_Date_must_be_in_the_future</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( AND ( NOT(ISNULL( cpm__Collection_Date__c ) ), cpm__Collection_Date__c &lt;= TODAY() ) , true, false )</errorConditionFormula>
        <errorDisplayField>cpm__Collection_Date__c</errorDisplayField>
        <errorMessage>Collection Date must be in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>cpm__Lock_Fiscal_Period</fullName>
        <active>true</active>
        <errorConditionFormula>cpm__Collection_Date__c &lt;= $Setup.cpm__Lock_Fiscal_Periods__c.cpm__Lock_Payments_Up_To__c</errorConditionFormula>
        <errorMessage>Fiscal period is closed. You can&apos;t edit this record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>cpm__Mandatory_Field_Check</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( OR ( ISBLANK(TEXT(cpm__Payment_Processor__c)) , ISBLANK (TEXT(cpm__PaymentMethod__c)) , ISBLANK(TEXT(cpm__Source__c)) ), true, false )</errorConditionFormula>
        <errorMessage>Payment Method, Payment Processor and Source are required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>paybacs__Target_is_mandatory_for_BACS</fullName>
        <active>true</active>
        <errorConditionFormula>IF ( AND ( 
								ISPICKVAL ( cpm__Payment_Processor__c,  &apos;PaymentHub-BACS&apos; ), 
								ISBLANK ( TEXT(cpm__Target__c) ) 
				) , true, false )</errorConditionFormula>
        <errorDisplayField>cpm__Target__c</errorDisplayField>
        <errorMessage>Target is mandatory for BACS Payment Schedules</errorMessage>
    </validationRules>
